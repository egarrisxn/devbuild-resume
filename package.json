{
  "name": "devbuild-resume",
  "version": "1.0.0",
  "description": "A typesafe resume builder built with Material-Tailwind, Nextjs, and more!",
  "homepage": "https://devbuild-resume.vercel.app",
  "author": "Ethan G",
  "license": "MIT",
  "private": true,
  "keywords": [
    "typescript",
    "next",
    "nextjs",
    "react",
    "tailwind",
    "tailwindcss",
    "material-tailwind",
    "eslint",
    "vercel"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "pnpm run lint",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "next-lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:fix": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "unrs-resolver"
    ]
  }
}
